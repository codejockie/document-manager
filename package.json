{
  "name": "document-manager",
  "version": "1.0.0",
  "description": "A RESTful API based document management system",
  "main": "server/server.js",
  "scripts": {
    "build": "webpack",
    "client:test": "jest --coverage",
    "client:test:coverage": "jest --coverage && cat ./coverage/lcov.info | codacy-coverage",
    "client:test:watch": "jest --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "debug": "nodemon --inspect --exec babel-node -- server/server.js",
    "gulp:compile": "gulp --require @babel/register compile",
    "heroku-postbuild": "yarn run gulp:compile && yarn run build",
    "migrate": "sequelize db:migrate && yarn run seed",
    "seed": "sequelize db:seed:all",
    "server:test": "NODE_ENV=test PORT=4000 nyc mocha -- ./__tests__/server/**/*.spec.js",
    "start": "babel-node server/server.js",
    "start:dev": "nodemon -w server --exec babel-node -- server/server.js",
    "test": "yarn run gulp:compile && yarn run server:test",
    "test:local": "yarn run test:undo && yarn run test:migrate && yarn test",
    "test:migrate": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "undo": "sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codejockie/document-manager.git"
  },
  "keywords": [
    "Document",
    "Docman",
    "DocManager",
    "Document Manager"
  ],
  "author": "John Kennedy (codejockie)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codejockie/document-manager/issues"
  },
  "homepage": "https://github.com/codejockie/document-manager#readme",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^8.0.0",
    "axios": "^0.17.1",
    "babel-loader": "^8.0.4",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.9",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-exit": "0.0.2",
    "gulp-inject-modules": "^1.0.0",
    "gulp-istanbul": "^1.1.2",
    "gulp-jasmine": "^2.4.2",
    "gulp-nodemon": "^2.2.1",
    "html-webpack-plugin": "^2.30.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.33",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.18.1",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.4.2",
    "nodemailer-express-handlebars": "^2.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.2.1",
    "style-loader": "^0.20.1",
    "validator": "^9.4.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "chai": "^4.1.0",
    "codacy-coverage": "^2.0.3",
    "coveralls": "^2.13.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.4.2",
    "mocha": "^5.0.1",
    "moxios": "^0.4.0",
    "nyc": "^11.4.1",
    "redux-mock-store": "^1.5.1",
    "sequelize-cli": "^2.7.0",
    "supertest": "^3.3.0"
  }
}
